        function loadAdminDataTable() {
            let currentDate = moment().utc().toISOString()
            let lastMonth = moment().subtract(30, 'days').utc().toISOString()
            let search = $('#select-role').val()
            let status = $('#admin-status').val()
            let start = $('#startDate').val()
            let end = $('#endDate').val()
            let last30Days = $('#last-30-days').val()
            let startDate = ''
            let endDate = ''

            $('#last-30-days').on('change', function () {
                if ($('#last-30-days').val() == 'Last 30 days') {
                    $('#startDate').attr('disabled', true)
                    $('#endDate').attr('disabled', true)
                } else {
                    $('#startDate').attr('disabled', false)
                    $('#endDate').attr('disabled', false)
                }
            })
            $('#startDate').change(function () {
                start = this.value
            })

            $('#endDate').on('change', function () {
                end = this.value
            })

            if (start && end) {
                startDate = moment(start, 'DD/MM/YYYY').format("YYYY-MM-DD");
                endDate = moment(end, 'DD/MM/YYYY').format("YYYY-MM-DD");
                if (!(new Date(startDate).getTime() <= new Date(endDate).getTime()) || new Date(startDate).getTime() >= new Date(moment()).getTime()) {
                    toastr.error('Selected date range is invalid.', 'Error')
                    $('#filter-name-role, #filter-name-status, #filter-lastMonth').addClass('hide')
                    return;
                }
            }

            if (last30Days) {
                startDate = moment(lastMonth).format("YYYY-MM-DD")
                endDate = moment(currentDate).format("YYYY-MM-DD")
            }

            $('#select-role').on('change', function () {
                search = this.value
            });

            $('#admin-status').on('change', function () {
                status = this.value
            })

            let generateUrl = getUrl(search, status, startDate, endDate)
            // Initiate datatable

