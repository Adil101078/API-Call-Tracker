        function createAdmin() {
            let countryCode = '+' + phoneInput.s.dialCode
            let data = {}
            let name = $('#admin_name').val().trim()
            let phone = $('#admin_number').val().replaceAll(/\s/g, '')
            let email = $('#admin_email').val()
            let nameRegex = /^[A-Z @~`!@#$%^&*()_=+\\\\';:\"\\/?>.<,-]*$/i
            let emailRegex = /(?=^(?:[A-Za-z0-9_\xC0-\xFF!#$%&'*+\/=?^`{|}~\\-]\.?){0,63}[A-Za-z0-9_\xC0-\xFF!#$%&'*+\/=?^`{|}~\\-]@[A-Za-z0-9\xC0-\xFF](?:[A-Za-z0-9\xC0-\xFF-]{0,61}[A-Za-z0-9\xC0-\xFF])?(?:\.[A-Za-z\xC0-\xFF](?:[A-Za-z0-9\xC0-\xFF-]{0,61}[A-Za-z0-9\xC0-\xFF])?)*$)(?=^.{3,254}$)/
            data.fullName = name
            data.mobile = phone
            data.email = email.toLowerCase()
            data.roleId = roleId
            data.countryCode = countryCode
            if (roleId === undefined) {
                $('#error-msg1').addClass('hide')
                $('#error-msg-role').text('Please select at least one role.')
                $('#error-msg-name').text('')
                $('#error-msg-phone').text('')
                $('#error-msg-email').text('')

            } else if (name.length < 3) {
                $('#error-msg1').addClass('hide')
                $('#error-msg-name').text('Admin name should be longer than 2 characters.')
                $('#error-msg-role').text('')
                $('#error-msg-phone').text('')
                $('#error-msg-email').text('')
            }
            else if (!nameRegex.test(name)) {
                $('#error-msg1').addClass('hide')
                $('#error-msg-name').text('Only alphabets and special characters are allowed.')
                $('#error-msg-role').text('')
                $('#error-msg-phone').text('')
                $('#error-msg-email').text('')
            }
            else if (!phone) {
                $('#error-msg1').addClass('hide')
                $('#error-msg-phone').text('Please enter phone number.')
                $('#error-msg-name').text('')
                $('#error-msg-role').text('')
                $('#error-msg-email').text('')
            }
            else if (!emailRegex.test(email)) {
                $('#error-msg1').addClass('hide')
                $('#error-msg-email').text('Please provide a valid email address.')
                $('#error-msg-name').text('')
                $('#error-msg-role').text('')
                $('#error-msg-phone').text('')
            }

            else {
                $.ajax({
                    url: `/api/v1/admin/createAdmin`,
                    type: 'POST',
                    data: JSON.stringify(data),
                    contentType: 'application/json',
                    success: function (data) {
                        $('#send-modal').modal('hide')
                        $('#modal-admin-success').modal('show')
                        loadAdminDataTable()
                    },
                    error: function (err) {
                        $('#error-msg-name').text('')
                        $('#error-msg-role').text('')
                        $('#error-msg-phone').text('')
                        $('#error-msg-email').text('')
                        $('#error-msg1').removeClass('hide')
                        $('#error-msg').text(err.responseJSON.msg)
                        $('#send-modal').on('hidden.bs.modal', function () {
                            $("#admin-create-form").trigger('reset')
                            $('#error-msg1').addClass('hide')
                        })
                    }
                })
            }
        }
        $('#edit-modal').on('hidden.bs.modal', function () {
            $('input[name="radio2"]').attr('checked', false)
            $('#edit-admin-data').trigger('reset')
            $('.accordion-toggle').addClass('collapsed')
            $('.collapse').removeClass('in')
        })

        $('#edit-modal').on('show.bs.modal', function (e) {
            $("#admin-name, #admin-email, #admin-number, input:radio").keyup(function () {
                if ($('#admin-name').val() !== '' && $('#admin-email').val() !== '' && $('#admin-number').val() !== '' && $("input[type='radio']").is(':checked')) {
                    $('#update_admin_details').prop('disabled', false)
                } else {
                    $('#update_admin_details').prop('disabled', true)
                }
            })
            $('.accordion-toggle').addClass('collapsed')
            let btn = $(e.relatedTarget)
            let edit_admin_id = btn.data('id')
            let countryCode = btn.data('countrycode')
            let name = btn.data('name')
            let email = btn.data('email')
            let phone = btn.data('phone')
            let role_id = btn.data('roleid')
            adminPhoneInput.setNumber(countryCode + phone)
            $("#admin-name").val(name)
            $("#admin-email").val(email)
            $("#country-code").text(countryCode)
            $("#admin-number").val(phone)
            $(`#${role_id}`).attr('checked', true)
            $(`input[name="radio2"]`).change(function () {
                $('#update_admin_details').prop('disabled', false)
                role_id = this.id
            })
            const radioButtons = document.querySelectorAll('input[name="radio2"]')
            for (const radioButton of radioButtons) {
                if (radioButton.checked) {
                    $('#update_admin_details').prop('disabled', false)

                }
                if (!radioButton.checked) {
                    $('#update_admin_details').prop('disabled', true)
                }
            }
            $('#update_admin_details').off().on('click', function () {
                let data = {}
                data.countryCode = '+' + adminPhoneInput.s.dialCode
                data.fullName = $("#admin-name").val().trim()
                data.mobile = $("#admin-number").val()
                data.email = $("#admin-email").val()
                data.roleId = role_id

                $('#update_admin_data').off().on('click', function () {
                    $.ajax({
                        url: `/api/v1/admin/update-admin/${edit_admin_id}`,
                        type: 'PATCH',
                        data: JSON.stringify(data),
                        contentType: 'application/json',
                        success: () => {
                            $('#edit-admin-success').modal('show')
                            loadAdminDataTable()
                        },
                        error: (err) => {
                            toastr.error(err.responseJSON.msg, "Error")
                        }
                    })

                })
            })





        })

    </script>
    <div class="modal coman-modal fade" id="modal-edit-success" tabindex="-1" role="dialog"
        aria-labelledby="myModalLabel">
        <div class="modal-dialog" role="document">
            <div class="modal-content text-center">
                <h2>Are you sure you want to edit the admin ?</h2>
                <div class="clearfix logout">
                    <button type="submit" class="btn   btn-default m-r-sm" data-dismiss="modal">No</button>
                    <button type="submit" id="update_admin_data" class="btn btn-primary" data-toggle="modal"
                        data-dismiss="modal">Yes</button>
                </div>
            </div>
        </div>
    </div>
    <div class="modal coman-modal  fade" id="edit-admin-success" tabindex="-1" role="dialog"
        aria-labelledby="myModalLabel">
        <div class="modal-dialog  modal-a" role="document">
            <div class="modal-content text-center">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
                <div class="success-msg-icon">
                    <i class="fa fa-check-circle m-b-0" aria-hidden="true"></i>
                </div>
                <h2> Admin Edited Successfully !!</h2>
            </div>
        </div>
    </div>

    </body>

    </html>